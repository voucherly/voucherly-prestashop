{
    "name": "voucherly/voucherly-prestashop",
    "version": "2.0.0",
    "author": "PrestaShop",
    "license": "OSL-3.0",
    "config": {
        "preferred-install": "dist",
        "prepend-autoloader": false
    },
    "require-dev": {
        "prestashop/autoindex": "^2.1",
        "spatie/ray": "^1.39",
        "prestashop/php-dev-tools": "^5.0",
        "friendsofphp/php-cs-fixer": "^3.65",
        "prestashop/header-stamp": "^2.3"
    },
    "scripts": {
        "fix-code": "vendor/bin/php-cs-fixer | vendor/bin/autoindex prestashop:add:index | vendor/bin/header-stamp --license=assets/afl.txt --exclude=vendor,node_modules"
    }
}
